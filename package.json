{
  "name": "extension-template-kitchensink",
  "version": "0.8.6",
  "description": "Looker Extension SDK functionality demonstration",
  "main": "dist/bundle.js",
  "scripts": {
    "analyze": "export ANALYZE_MODE=static && yarn build",
    "build": "export BABEL_ENV=build && webpack",
    "clean": "rm -rf dist",
    "develop": "webpack serve --hot --port 8080 --disable-host-check --config webpack.develop.js",
    "prebuild": "yarn clean",
    "lint": "run-p -c lint:*",
    "lint:ts": "tsc --skipLibCheck --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "data-server": "cp db.json temp_db.json && nodemon server/index.js"
  },
  "author": "Looker",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@looker/components": "^0.9.25",
    "@looker/embed-sdk": "^1.4.0",
    "@looker/extension-sdk": "^0.13.1",
    "@looker/extension-sdk-react": "^0.7.1",
    "@looker/sdk": "^0.3.7-beta.5",
    "@looker/sdk-rtl": "^0.3.7-beta.5",
    "axios": "^0.19.2",
    "date-fns": "^2.12.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "semver": "^7.3.4",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.12",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.5",
    "@types/readable-stream": "^2.3.5",
    "@types/semver": "^7.3.1",
    "@types/styled-components": "5.1.5",
    "babel-loader": "^8.2.2",
    "babel-preset-nano-react-app": "^0.1.0",
    "dotenv": "^8.2.0",
    "json-server": "^0.16.3",
    "minimist": "^1.2.2",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.12.20",
    "typescript": "4.1.2",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.10.3"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  }
}
